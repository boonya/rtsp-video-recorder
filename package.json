{
  "name": "rtsp-video-recorder",
  "version": "1.2.0-alpha.1",
  "description": "Provide an API to record rtsp video stream to filesystem.",
  "main": "dist/recorder.js",
  "types": "dist/recorder.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/boonya/rtsp-video-recorder"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=6"
  },
  "scripts": {
    "start": "npm run lint && npm run test && npm run build",
    "dev": "./node_modules/.bin/tsc --watch",
    "build": "./node_modules/.bin/tsc",
    "test": "./node_modules/.bin/jest",
    "lint": "./node_modules/.bin/tslint --project tsconfig.json --config tslint.json --format stylish",
    "lint:prod": "./node_modules/.bin/tslint --project tsconfig.json --config tslint.prod.json --format stylish"
  },
  "keywords": [
    "rtsp",
    "video",
    "recorder",
    "stream",
    "webcam"
  ],
  "author": "Serhii [boonya] Buinytskyi",
  "license": "MIT",
  "dependencies": {
    "du": "^1.0.0",
    "fs-extra": "^8.1.0",
    "strftime": "^0.10.0"
  },
  "devDependencies": {
    "@types/du": "^1.0.0",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.25",
    "@types/node": "^12.12.24",
    "@types/strftime": "^0.9.2",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.7.4"
  }
}
